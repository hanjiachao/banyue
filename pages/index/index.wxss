}
/*遮罩*/
.shade{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background:rgba(0,0,0,0.5);
  z-index: 999;
}
.shade .conts{
  width: 440rpx;
  margin: 250rpx auto 0;
  position: relative;
}
.shade .conts .bg{
  width: 100%;
}
.shade .conts .panel{
  position: absolute;
  top: 240rpx;
  left: 0;
  right: 0;
  text-align: center;
}
.shade .conts .panel .title{
  color: #1C1C1C;
  font-size: 32rpx;
  font-weight: 600;
}
.shade .conts .panel .test{
  color: #808080;
  font-size: 24rpx;
  margin-top: 30rpx;
}
.shade .conts .panel .btn{
  background: #0BCA9E;
  width: 280rpx;
  color: #fff;
  font-size: 24rpx;
  margin-top: 60rpx;
}
.shade .conts .cancel{
  width: 50rpx;
  height: 50rpx;
  margin: 40rpx auto 0;
}
.shade .conts .cancel image{
  width: 100%;
}