/**index.wxss**/
.container{
  overflow: hidden;
}
.header{
  background: #03CA9E;
  padding: 15rpx;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 999;
}
.header>image{
  width: 45rpx;
  height: 45rpx;
  margin-top: 5px;
}
.header .center{
  margin: 0 70rpx;
  background: #fff;
  height: 65rpx;
  line-height: 65rpx;
  border-radius: 32rpx;
  overflow: hidden;
}
.header .center image{
  width: 27rpx;
  height: 27rpx;
  margin-top: 18rpx;
  margin-left: 28rpx;
}
.header .center .ipt{
  margin-left: 70rpx;
  height: 100%;
  color: #CCCCCC;
  font-size: 28rpx;
}
.slide-image{
  width:100%;
  height: 100%;
}
.content{
  width: 100%;
  background: #F2F3F4;
  overflow: hidden;
  padding-bottom: 20rpx
}
.content .nav{
  margin: 25rpx;
  padding: 40rpx 30rpx;
  background: #ffffff;
  border-radius: 20rpx;
  box-shadow: 2rpx 2rpx 5rpx #999;
  box-sizing: border-box;
}
.content .nav .item{
  width: 25%;
  float: left;
  text-align: center;
  padding: 10rpx 5rpx;
  box-sizing: border-box;
}
.content .nav .item image{
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
}
.content .nav .item text{
  display: block;
  color: #464646;
  font-size: 26rpx;
  font-weight: 500;
  text-align: center;
  margin-top: 20rpx;
}
.content .panel{
  margin: 40rpx 25rpx;
}
.content .panel image{
  width: 125rpx;
  height: 90rpx;
}
.content .panel .info{
  margin-left: 150rpx;
  font-size: 24rpx;
  line-height: 45rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.content .panel .info .icon{
  width: 10rpx;
  height: 10rpx;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: middle;
}
.content .panel .title{
  margin: 20rpx 0;
  line-height: 30rpx;
}
.content .panel .left{
  font-weight: 600;
  font-size: 28rpx
}
.content .panel .title .link{
  font-size: 24rpx;
  color: #A6A6A6;
}
.content .course{
  background: #fff;
  margin-top: 30rpx;
  padding: 0 25rpx;
}
.content .course>image{
  width: 100%;
  height: auto;
}
.content .course .boutique .item{
  width: 48%;
  background: #EAF7AF;
  padding: 40rpx 35rpx;
  margin-top: 25rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  vertical-align: top;
}
.content .course .boutique .item:nth-child(2n+1){
  float: left;
}
.content .course .boutique .item:nth-child(2n){
  float: right;
}
.content .course .boutique .item:nth-child(2){
  background: #FFDAB3;
}
.content .course .boutique .item:nth-child(3){
  background: #B3E7FF;
}
.content .course .boutique .item:nth-child(4){
  background: #FFC3C3;
}
.content .boutique .item .details{
  font-size: 26rpx;
  height: 60rpx;
  line-height: 30rpx;
  color: #333333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.content .boutique .item .money{
  color: #FB524F;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.content .boutique .numberBox{
  vertical-align: top;
  overflow: hidden;
}
.content .boutique .numberBox image{
  width: 25rpx;
  height: 25rpx;
}
.content .boutique .numberBox text{
  color: #333333;
  font-size: 26rpx;
  line-height: 30rpx;
  margin-left: 5rpx;
}
.content .courseList{
  margin-top: 20rpx;
}
.content .courseList .item{
  padding: 20rpx 0;
  border-bottom: 2rpx solid #DBDBDB;
}
.content .courseList .item:last-child{
  border: 0px;
}
.content .courseList .item .left{
  width: 125rpx;
  height: 125rpx;
  overflow: hidden;
}
.content .courseList .item .left image{
  width: 100%;
  height: 100%;
}
.content .courseList .info{
  margin-left: 150rpx;
}
.content .courseList .info .title{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  color: #333333;
  font-size: 28rpx;
  line-height: 30rpx;
}
.content .courseList .number{
  color: #8B8B8B;
}
.content .courseList .number image{
  width: 25rpx;
  height: 25rpx;
}
.content .courseList .number .people{
  margin-left: 10rpx;
  font-size: 26rpx;
  line-height: 30rpx;
}
.content .courseList .money{
  margin-top: 15rpx;
}
.content .courseList .money .team{
  color:#FF4D4D; 
  float: left;
  font-size: 26rpx;
  line-height: 30rpx;
}
.content .courseList .money .unit{
  color: #888888;
  margin-left: 10rpx;
  font-size: 26rpx;
  line-height: 30rpx;
  float: left;
}
.content .course .btn{
  text-align: center;
  color: #888888;
  font-size: 28rpx;
  line-height: 30rpx;
  padding-bottom: 30rpx;
}
