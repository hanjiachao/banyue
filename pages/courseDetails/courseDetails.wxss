/* 章节 */
.listBox{
  padding: 40rpx 0;
}
.listBox .title{
  color: #333333;
  font-size: 30rpx;
  font-weight: 500;
  margin-left: 25rpx;
}
.listBox .list{
  padding: 30rpx 50rpx;
}
.listBox .list .item{
  height: 80rpx;
  border-bottom: 2rpx solid #E1E2E3;
}
.listBox .list .item .play-img{
  width: 22rpx;
  height: 25rpx;
  margin-top: 30rpx;
}
.listBox .list .item .play-img image{
  width: 100%;
  height: 100%;
  display: block;
}
.listBox .list .item .timer{
  color: #999999;
  font-size: 26rpx;
  line-height: 80rpx;
}
.listBox .list .item .text-cen{
  color: #666;
  font-size: 26rpx;
  line-height: 80rpx;
  margin: 0 165rpx 0 45rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.speedContent{
	width: 90rpx;
	position: absolute;
	right: 0;
	top: 0;
}
.speedContent.active{
	background: rgba(0,0,0,0.2);
}
.moreBtn{
	width: 32rpx;
	height: 32rpx;
	padding: 30rpx 30rpx 20rpx;
}
.speedList{
	margin-top: -15rpx;
	padding-bottom: 10rpx;
}
.speedList .item{
	width: 90rpx;
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	color: #fff;
	font-size: 24rpx;
	font-weight: bold;
}
.videoBtns{
	display: flex;
	justify-content: center;
	align-items: center;
	border-bottom: 2rpx solid #E1E2E3;
}
.videoBtns .btn{
	width: 50%;
	line-height: 60rpx;
	padding-bottom: 10rpx;
	text-align: center;
	font-size: 32rpx;
	color: #333;
}