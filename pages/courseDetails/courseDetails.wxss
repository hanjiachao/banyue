/* pages/courseDetails/courseDetails.wxss */
.conts .header{
  background: #fff;
}
.conts .header .videoBox{
  position: relative;
  width: 100%;
}
.conts .header .videoBox video{
  width: 100%;
}
.conts .header .videoBox .shade{
  position: relative;
  background: #000;
  z-index: 999;
}
.conts .header .title{
  color: #333333;
  font-size: 36rpx;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  padding: 20rpx 25rpx 0;
  font-weight: 600;
}
.conts .header .info{
  color: #666666;
  font-size: 28rpx;
  padding: 10rpx 25rpx 20rpx;
}
.conts .header .money{
  color: #FF4848;
  font-size: 36rpx;
  font-weight: bold;
  padding: 0 25rpx 20rpx;
  margin-top: 25rpx;
}
.conts .header .team{
  color: #FE6060;
  font-size: 26rpx;
  line-height: 50rpx;
  padding: 0 25rpx 20rpx;
}
.conts .header .team .num1{
  margin-right: 80rpx;
}
.conts .msg{
  background: #fff;
  margin: 20rpx 0;
}
.conts .msg .list{
  padding: 40rpx 0 20rpx;
  border-bottom: 2rpx solid #F2F2F2;
}
.conts .msg .list:last-child{
  border-bottom: 0;
}
.conts .msg .list .info{
  display: inline-block;
  width: 50%;
  text-align: center;
  border-right: 2rpx solid #F2F2F2;
  padding-left: 55rpx;
  box-sizing: border-box;
}
.conts .msg .info .img{
  width: 40rpx;
  height: 35rpx;
  margin-top: 20rpx;
}
.conts .msg .info .img image{
  width: 100%;
  height: 100%;
}
.conts .msg .info .text{
  margin-left: 20rpx;
  text-align: left;
}
.conts .msg .info .title{
  color: #333333;
  font-size: 30rpx;
  line-height: 40rpx;
}
.conts .msg .info .num{
  color: #666;
  font-size: 26rpx;
  line-height: 40rpx;
}
.conts .teamRule{
  background: #fff;
  margin-bottom: 20rpx;
  padding: 25rpx;
}
.conts .teamRule .title{
  color: #333333;
  font-size: 32rpx;
}
.conts .teamRule .info{
  margin-top: 20rpx;
  color: #666666;
  font-size: 28rpx;
  line-height: 40rpx;
}
.conts .tab{
  background: #fff;
  margin-bottom: 97rpx;
}
.conts .tab .head{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #E1E2E3;
}
.conts .tab .head .items{
  display: inline-block;
  width: 33.33%;
  text-align: center;
  color: #333333;
  font-size: 32rpx;
  position: relative;
}
.conts .tab .head .show{
  color: #0BC99D;
}
.conts .tab .show .active{
  position: absolute;
  width: 125rpx;
  height: 2rpx;
  background: #0BC99D;
  left: 60rpx;
  bottom: 5rpx;
}
swiper{
  background: #fff;
}
swiper scroll-view{
  height: 100%;
}
.scroll .list{
  background: #fff;
  padding: 40rpx 25rpx;
}
swiper .list .user .hdImg{
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  overflow: hidden;
}
swiper .list .user .hdImg image{
  width: 100%;
  height: 100%;
}
swiper .list .user .info{
  margin-left: 160rpx;
}
swiper .list .user .info .name{
  color: #333333;
  font-size: 24rpx;
  line-height: 40rpx;
}
swiper .list .user .info .text{
  color: #666666;
  font-size: 24rpx;
  line-height: 40rpx;
  margin-top: 15rpx;
}
swiper .list .wrap{
  border-top: 20rpx solid #F2F3F4;
  background: #fff;
  padding: 30rpx 25rpx;
}
swiper .list .wrap .title{
  color: #333333;
  font-size: 24rpx;
}
swiper .list .wrap .text{
  color: #666666;
  font-size: 24rpx;
  margin-top: 30rpx;
}
swiper .section{
  padding: 40rpx 25rpx;
}
swiper .section .title{
  color: #333333;
  font-size: 30rpx;
}
swiper .section .info .text{
  color: #333333;
  font-size: 26rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
}
swiper .section .info .content{
  border-bottom: 2rpx solid #E1E2E3;
}
swiper .section .info .content .msg{
  margin: 0;
}
swiper .section .info .content .msg .icon{
  width: 22rpx;
  height: 25rpx;
  display: inline-block;
  margin: 0 10rpx 0 70rpx;
}
swiper .section .info .content .msg .icon image{
  width: 100%;
  height: 100%;
}
swiper .section .info .content .msg .test{
  display: inline-block;
  color: #666666;
  font-size: 26rpx;
}
swiper .section .info .content .timer{
  color: #999999;
  font-size: 26rpx;
  line-height: 70rpx;
}
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100rpx;
  border-top: 2rpx solid #DDDDDD;
  text-align: center;
  background: #fff;
}
.footer .operation{
  width: 50%;
  color: #666666;
  font-size: 28rpx;
  height: 100%;
}
.footer .operation .content{
  width: 50%;
  padding-top: 15rpx;
  display: inline-block;
  border-right: 2rpx solid #DDDDDD;
  box-sizing: border-box;
  height: 100%;
}
.footer .operation .content .icon{
  width: 40rpx;
  height: 35rpx;
  display: inline-block;
}
.footer .operation .content .icon image{
  width: 100%;
  height: 100%;
}
.footer .btn{
  width: 50%;
  background: #00C497;
  color: #FFFFFF;
  font-size: 36rpx;
  height: 100%;
  line-height: 95rpx;
  text-align: center;
}
.footer .btn>view{
  width: 50%;
  display: inline-block;
}
.footer .btn .title{
  color: #FFFFFF;
  font-size: 30rpx;
}
.footer .btn .text{
  color: #FFFFFF;
  font-size: 26rpx;
}
.footer .btn .money1{
  background: #FF4848;
  line-height: 45rpx;
  height: 100%;
}
.footer .btn .money2{
  background: #00C497;
  line-height: 45rpx;
  height: 100%;
}
