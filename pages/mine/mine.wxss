/* pages/mine/mine.wxss */
.header{
  padding: 60rpx 40rpx;
  background-image:url('data:image/png;base64,');
  background-color: #ffffff;
  background-size: 100% 100%;
}
.header .headPortrait{
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  overflow: hidden;
}
.header .headPortrait image{
  width: 100%;
}
.header .signin{
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  position: relative;
  background: #fff;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
}
.header .signin .icon{
  width: 24rpx;
  height: 24rpx;
  margin-left: 20rpx;
  align-items: center;
}
.header .signin text{
  color:#00B087;
  font-size:26rpx;
  margin-left: 20rpx;
}
.header .test{
  margin: 20rpx 130rpx 0;
  color: #ffffff;
}
.header .test .title{
  font-size: 32rpx;
}
.header .test .title image{
  width: 26rpx;
  height: 26rpx;
  margin-left: 20rpx;
}
.header .test .info{
  font-size: 24rpx;
}
.header .test .info .time{
  margin-right: 20rpx;
}
.list{
  padding: 20rpx 20rpx;
  background: #ffffff;
  box-sizing: border-box;
}
.list .item{
  width: 25%;
  padding: 20rpx;
  box-sizing: border-box;
  text-align: center;
}
.list .item image{
  width: 92rpx;
  height: 92rpx;
}
.list .item text{
  display: block;
  font-size: 24rpx;
  color: #333333;
  margin-top: 20rpx;
}
.navList{
  margin: 20rpx 0;
  background: #fff;
  padding: 0 20rpx;
}
.navList .item{
  padding: 20rpx;
  line-height: 40rpx;
  border-bottom: 2rpx solid  #F3F3F3 ;
}
.navList .item:last-child{
  border-bottom: 0;
}
.navList .item image{
  width: 31rpx;
  height: 38rpx;
  vertical-align: middle;
}
.navList .item text{
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #333333;
}

/*遮罩*/
.shade{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background:rgba(0,0,0,0.5);
  z-index: 999;
}
.shade .conts{
  width: 440rpx;
  margin: 250rpx auto 0;
  position: relative;
}
.shade .conts .bg{
  width: 100%;
}
.shade .conts .panel{
  position: absolute;
  top: 240rpx;
  left: 0;
  right: 0;
  text-align: center;
}
.shade .conts .panel .title{
  color: #1C1C1C;
  font-size: 32rpx;
  font-weight: 600;
}
.shade .conts .panel .test{
  color: #808080;
  font-size: 24rpx;
  margin-top: 30rpx;
}
.shade .conts .panel .btn{
  background: #0BCA9E;
  width: 280rpx;
  color: #fff;
  font-size: 24rpx;
  margin-top: 60rpx;
}
.shade .conts .cancel{
  width: 50rpx;
  height: 50rpx;
  margin: 40rpx auto 0;
}
.shade .conts .cancel image{
  width: 100%;
}
.shade .content{
  width: 600rpx;
  margin: 290rpx auto 0;
  background: #fff;
  border-radius: 10rpx;
  text-align: center;
  overflow: hidden;
  
}
.shade .content .title{
  margin-top: 80rpx;
  color: #333333;
  font-size: 30rpx;
}
.shade .content .btn{
  margin: 90rpx 0 40rpx;
}
.shade .content button{
  color: #fff;
  font-size: 32rpx;
  width: 250rpx;
  display: inline-block;
}
.shade .content .close{
  background: #CCCCCC;
  margin-right: 30rpx;
}
.shade .content .apply{
  background: #0BC99D;
}