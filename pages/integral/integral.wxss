/* pages/integral/integral.wxss */
@import "../../wxParse/wxParse.wxss";
page{
  background-image:url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-color: #fff; 
  max-height: 100%;
}
.conts{
  padding: 0 25rpx;
}
.conts .header{
  padding: 25rpx 0;
  color: #fff;
}
.conts .header .title{
  height: 60rpx;
  line-height: 60rpx;
}
.conts .header .title .integral{
  font-size: 28rpx;
}
.conts .title .details{
  font-size: 24rpx;
  overflow: hidden;
}
.conts .title .details image{
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.conts .header .num{
  font-size: 72rpx;
  margin-top: 10rpx;
  text-align: left;
  font-weight: 600;
}
.conts .header .num text{
  font-size: 28rpx;
  font-weight: 500;
}
.conts .panel{
  margin-top: 90rpx;
}
.conts .panel .title{
  color: #333333;
  font-size: 32rpx;
}
.conts .panel .list{
  padding: 20rpx 0;
  border-bottom: 2rpx solid #EEEEEE;
  height: 100rpx;
}
.conts .panel .list:last-child{
  border-bottom: 0;
}
.conts .panel .list .info .head{
  color: #333333;
  font-size: 32rpx;
  line-height: 50rpx;
}
.conts .panel .list .info .timer{
  color: #5A5A5A;
  font-size: 24rpx;
  line-height: 50rpx;
}
.conts .panel .list .number{
  color: #333333;
  font-size: 32rpx;
  line-height: 100rpx;
}
.shade{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .5);
  z-index: 999;
}
.shade .panel{
  width: 625rpx;
  margin: 125rpx auto 0;
  position: relative;
}
.shade .panel .bg{
  width: 100%;
}
.shade .panel .info{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  padding: 40rpx;
  text-align: center;
}
.shade .panel .info .title{
  color: #333333;
  font-size: 32rpx;
}
.shade .panel .info .item{
  color: #333333;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: left;
  margin-top: 35rpx;
}
.shade .icon{
  width: 60rpx;
  height: 60rpx;
  margin: 60rpx auto 0;
}
.shade .icon image{
  width: 100%;
  height: 100%;
}